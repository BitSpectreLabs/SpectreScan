rarity 4
totalwaitms 5000

match mysql m|.\x00\x00\x00\n(\d+\.\d+\.\d+)|s p/MySQL/ v/$1/ cpe:/a:mysql:mysql:$1/
match mysql m|MariaDB| p/MariaDB/

